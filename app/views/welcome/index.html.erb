<script>
	$(document).ready(function(){
		$("#home").show();
		$("#form").hide();
		$("#contact").hide();
	});
	function showInicio(){
			$("#home").show();
			$("#form").hide();
			$("#contact").hide();
	}
	function showForm(){
		$("#home").hide();
		$("#form").show();
		$("#contact").hide();
	}
	
	function showContact(){
		$("#home").hide();
		$("#form").hide();
		$("#contact").show();
	}
</script>




